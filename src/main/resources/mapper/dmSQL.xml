<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="dm">
	<insert id="sendDm" parameterType="dm">
		insert into dm
		values(
		dm_seq.nextval,
		#{sender},
		#{receiver},
		#{dmContent},
		sysdate,
		0
		)
	</insert>
</mapper>